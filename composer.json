{
    "name": "juzaweb/multiple-storage",
    "type": "juzaweb-plugin",
    "license": "MIT",
    "description": "Multiple storage support for juzaweb",
    "extra": {
        "juzaweb": {
            "name": "Multiple storage for juzaweb",
            "domain": "multi_storage",
            "cms_min": "3.4",
            "providers": [
                "Juzaweb\\MultipleStorage\\Providers\\MultipleStorageServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\MultipleStorage\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "league/flysystem-ftp": "^3.15",
        "league/flysystem-sftp-v3": "^3.15",
        "spatie/flysystem-dropbox": "^3.0"
    },
    "require-dev": {
        "juzaweb/modules": "^1.0"
    }
}
